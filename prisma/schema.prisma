generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model activeSites {
  id          String   @id @default(cuid())
  website     String
  apiEndpoint String
  recurrence  String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
